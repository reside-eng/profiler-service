{
  "name": "rtdb-profiler-service",
  "version": "1.0.0",
  "description": "Profiles Firebase's Real-time Database and logs it's output",
  "main": "lib/index.js",
  "engines": {
    "node": "^12.x.x"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "start": "node lib/index.js",
    "dev": "npm run build && npm run start",
    "lint": "eslint '**/*.{js,ts}' .eslintrc.js --ignore-pattern='!.eslintrc.js'",
    "lint:fix": "npm run lint -- --fix"
  },
  "author": "reside-eng",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{json,md}": [
      "prettier --write"
    ],
    "*.{js,ts}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@google-cloud/logging": "^7.1.0",
    "date-fns": "^2.9.0",
    "firebase-admin": "^8.9.2",
    "firebase-tools": "^7.13.0"
  },
  "devDependencies": {
    "@side-eng/eslint-config-base": "0.0.1",
    "@side-eng/eslint-config-prettier": "0.0.1",
    "@types/express": "^4.17.2",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.5.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "eslint": "^6.8.0",
    "firebase-functions-test": "^0.1.6",
    "husky": "4.2.3",
    "lint-staged": "10.0.8",
    "prettier": "^1.19.1",
    "typescript": "^3.2.2"
  }
}
